{
  "name": "remix-prisma-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "format": "prettier --write \"app/**/*.*\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "db:migrate": "prisma-case-format --file ./prisma/schema.prisma && prisma migrate dev && prisma generate && npm run db:deploy",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "test": "dotenv -e .env.test -- vitest run",
    "test:cov": "dotenv -e .env.test -- vitest run --coverage",
    "test:db:reset": "dotenv -e .env.test -- prisma migrate reset -f"
  },
  "dependencies": {
    "@conform-to/react": "^1.1.5",
    "@conform-to/zod": "^1.1.5",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@epic-web/remember": "^1.1.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.1",
    "@prisma/client": "^5.16.1",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "@remix-run/serve": "^2.10.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@onozaty/prisma-db-comments-generator": "^1.0.1",
    "@quramy/prisma-fabbrica": "^2.2.1",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/testing": "^2.11.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.2",
    "prisma": "^5.16.1",
    "prisma-case-format": "^2.2.1",
    "remix-flat-routes": "^0.6.5",
    "tsx": "^4.16.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
